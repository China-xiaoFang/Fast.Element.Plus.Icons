<Project Sdk="Microsoft.NET.Sdk.Web">

	<PropertyGroup>
		<TargetFramework>net6.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>disable</Nullable>
		<GenerateDocumentationFile>True</GenerateDocumentationFile>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<IncludeSymbols>true</IncludeSymbols>
		<SymbolPackageFormat>snupkg</SymbolPackageFormat>
		<PackageOutputPath>$(SolutionDir)..\..\nupkgs</PackageOutputPath>
		<Version>3.0.9-test.1.20231024.1</Version>
		<PackageVersion>$(Version)</PackageVersion>
		<Authors>1.8K仔</Authors>
		<Description>一个快速构建项目的 .NET 框架。</Description>
		<PackageTags>Fast;ASP.NET;dotNET China;Cross Platform;1.8K仔</PackageTags>
		<Product>Fast</Product>
		<Copyright>版权所有 © 2018-2023 1.8K仔</Copyright>
		<PackageProjectUrl>https://18kboy.icu</PackageProjectUrl>
		<PackageIcon>Fast.jpg</PackageIcon>
		<PackageReadmeFile>README.md</PackageReadmeFile>
		<RepositoryUrl>https://gitee.com/Net-18K/Fast.NET</RepositoryUrl>
		<RepositoryType>Gitee</RepositoryType>
		<PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
		<DocumentationFile>$(MSBuildProjectName).xml</DocumentationFile>
		<GenerateResxSourceIncludeDefaultValues>true</GenerateResxSourceIncludeDefaultValues>
	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
		<NoWarn>1701;1702;1591</NoWarn>
	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
		<NoWarn>1701;1702;1591</NoWarn>
	</PropertyGroup>

	<ItemGroup>
    <Compile Remove="logs\**" />
    <Content Remove="logs\**" />
    <EmbeddedResource Remove="logs\**" />
    <None Remove="logs\**" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="RabbitMQ.Client" Version="6.6.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\Fast.NET\Cache\Fast.Cache\Fast.Cache.csproj" />
    <ProjectReference Include="..\..\Fast.NET\Core\Fast.NET.Core\Fast.NET.Core.csproj" />
    <ProjectReference Include="..\..\Fast.NET\CorsAccessor\Fast.CorsAccessor\Fast.CorsAccessor.csproj" />
    <ProjectReference Include="..\..\Fast.NET\DependencyInjection\Fast.DependencyInjection\Fast.DependencyInjection.csproj" />
    <ProjectReference Include="..\..\Fast.NET\EventBus\Fast.EventBus\Fast.EventBus.csproj" />
    <ProjectReference Include="..\..\Fast.NET\Fast.SqlSugar\Fast.SqlSugar.csproj" />
    <ProjectReference Include="..\..\Fast.NET\IaaS\Fast.IaaS\Fast.IaaS.csproj" />
    <ProjectReference Include="..\..\Fast.NET\JwtBearer\Fast.JwtBearer\Fast.JwtBearer.csproj" />
    <ProjectReference Include="..\..\Fast.NET\Logging\Fast.Logging\Fast.Logging.csproj" />
    <ProjectReference Include="..\..\Fast.NET\Mapster\Fast.Mapster\Fast.Mapster.csproj" />
    <ProjectReference Include="..\..\Fast.NET\Serialization\Fast.Serialization\Fast.Serialization.csproj" />
    <ProjectReference Include="..\..\Fast.NET\SpecificationProcessor\Fast.SpecificationProcessor\Fast.SpecificationProcessor.csproj" />
  </ItemGroup>

</Project>
